<?xml version="1.0" encoding="utf-8"?>
<layout>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/innerpage_body">

    <ScrollView style="@style/innerpage_body_scroll">

        <LinearLayout style="@style/innerpage_scrollin">

            <LinearLayout style="@style/innerpage_head">

                <ImageView
                    android:id="@+id/im_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"
                    android:layout_marginLeft="15dp"
                    android:layout_marginStart="15dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:padding="10dp"

                    android:src="@drawable/back_icon" />

                <ImageView
                    style="@style/innerpage_headico"
                    android:src="@drawable/load_expense_icon" />

                <feedback.mpnsc.CustomTextViewThin
                    style="@style/innerpage_headtitle"
                    android:text="Load Expenses " />

            </LinearLayout>

            <LinearLayout style="@style/innerpage_content">


                <TableLayout
                    android:id="@+id/tab_ticket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tab_reg"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp">

                    <TableRow>

                        <TextView
                            android:id="@+id/textViewTicketValue"
                            style="@style/label_title"
                            android:text="Ticket Number : " />

                        <TextView
                            android:id="@+id/textViewTicket"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_column="1"
                            android:padding="5dp"
                            android:text=""
                            android:textColor="#FFF"
                            android:textSize="22dp" />


                    </TableRow>

                </TableLayout>


                <LinearLayout
                    android:id="@+id/linear_bulbs"
                    style="@style/linear_label_holder">

                    <TextView
                        android:id="@+id/tv_bulbs"
                        style="@style/label_title"

                        android:text="LIGHTING BULBS" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:orientation="horizontal"
                        android:weightSum="2">

                        <EditText
                            android:id="@+id/et_bulbNo"
                            style="@style/input_gen"
                            android:layout_weight="1"
                            android:hint="Numbers"
                            android:inputType="numberSigned"
                            android:layout_margin="2dp"
                            android:maxLength="35" />

                        <EditText
                            android:id="@+id/et_bulbEnergy"
                            style="@style/input_gen"
                            android:layout_weight="1"
                            android:hint="KW"
                            android:layout_margin="2dp"
                            android:inputType="numberSigned"

                            android:maxLength="35" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear_fan"
                    style="@style/linear_label_holder">

                    <TextView
                        android:id="@+id/tv_fan"
                        style="@style/label_title"

                        android:text="Fans" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:orientation="horizontal"
                        android:weightSum="2">

                        <EditText
                            android:id="@+id/et_fanNo"
                            style="@style/input_gen"
                            android:layout_weight="1"
                            android:hint="Numbers"
                            android:inputType="numberSigned"
                            android:layout_margin="2dp"
                            android:maxLength="35" />

                        <EditText
                            android:id="@+id/et_fanEnergy"
                            style="@style/input_gen"
                            android:layout_weight="1"
                            android:hint="kw"
                            android:layout_margin="2dp"
                            android:inputType="numberSigned"

                            android:maxLength="35" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear_fridge"
                    style="@style/linear_label_holder">

                    <TextView
                        android:id="@+id/tv_fridge"
                        style="@style/label_title"

                        android:text="Fridge/DeepFreezer" />

                    <LinearLayout
